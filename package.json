{
   "name": "powco-api",
   "version": "0.2.2",
   "private": true,
   "description": "Boost Proof of Work Platform API at https://Pow.Co",
   "scripts": {
      "test": "NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require=ts-node/register --timeout=20000 test/**/*_test.ts --exit",
      "runtest": "NODE_ENV=test mocha --require=ts-node/register --timeout=20000 $1",
      "start": "ts-node src/main.ts",
      "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
      "cover:unit": "nyc --silent npm run test:unit",
      "cover:integration": "nyc --silent --no-clean npm run test:integration",
      "cover:report": "nyc report --reporter=lcov --reporter=text"
   },
   "nyc": {
      "check-coverage": false,
      "all": true,
      "include": [
         "src/*.ts",
         "src/**/*.ts",
         "actors/**/*.ts",
         "planaria/**/*.ts"
      ],
      "exclude": [
         "includes/**.ts"
      ],
      "reporter": "lcov",
      "report-dir": "./coverage"
   },
   "keywords": [],
   "author": "Steven Zeiler",
   "license": "BSV",
